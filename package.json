{
  "name": "react-typescript",
  "description": "A boilerplate for React with TypeScript projects.",
  "version": "0.0.2",
  "license": "MIT",
  "author": "sprout2000",
  "homepage": "https://sprout2000.github.io/react-typescript",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sprout2000/react-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/sprout2000/react-typescript/issues"
  },
  "keywords": [
    "jest",
    "react",
    "webpack",
    "typescript",
    "boilerplate",
    "@testing-library"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=\"development\" webpack serve",
    "build": "cross-env NODE_ENV=\"production\" webpack --progress",
    "test": "jest",
    "lint-staged": "lint-staged",
    "fix:eslint": "eslint --ext .js,.ts,.jsx,.tsx --fix . && prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "npx eslint --fix .",
      "npx prettier --write ."
    ]
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@jest/types": "^26.6.2",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/css-minimizer-webpack-plugin": "^1.1.1",
    "@types/jest": "^26.0.20",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/node": "^14.14.33",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/terser-webpack-plugin": "^5.0.2",
    "@types/webpack-dev-server": "^3.11.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.2",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.3.9",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2",
    "webpack": "^5.24.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1"
  }
}
